.savanna-styles .back { overflow-wrap: break-word; transform: rotateY(180deg) translate(100%, 0%); background: linear-gradient(to right, rgb(0, 43, 86), 40%, rgb(2, 142, 202)); color: white; justify-content: left; }

.savanna-styles .front { background-color: rgb(0, 43, 86); color: rgb(5, 242, 131); justify-content: center; display: flex; align-items: center; text-align: center; }

