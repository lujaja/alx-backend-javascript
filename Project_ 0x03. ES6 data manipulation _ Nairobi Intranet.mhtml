Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: https://alx-tools.github.io/savanna-content-styles/assets/css/styles.css

@charset "utf-8";

@import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap");

.savanna-styles { color: rgb(0, 43, 86); font-family: Poppins, "sans-serif"; }

.savanna-styles h { font-weight: bold; }

.savanna-styles a { color: rgb(2, 142, 202); }

.savanna-styles a:hover { color: rgb(2, 142, 202); }

.savanna-styles a:visited { color: rgb(86, 72, 183); }

.savanna-styles mark { color: rgb(0, 43, 86); background-color: rgb(5, 242, 131); font-weight: bold; display: inline-block; padding-right: 5px; padding-left: 5px; }

.savanna-styles .bold-secondary { color: rgb(5, 242, 131); font-weight: bold; }

.savanna-styles .bold-tertiary { color: rgb(2, 142, 202); font-weight: bold; }

.savanna-styles .bold-accent { color: rgb(86, 72, 183); font-weight: bold; }

.savanna-styles figure figcaption { text-align: center; line-height: 20px; }

.savanna-styles .reflection-element { background-color: rgb(221, 218, 241); padding: 20px; text-align: left; border: 3px solid rgb(86, 72, 183); border-radius: 5px; }

.savanna-styles .reflection-element h { color: rgb(86, 72, 183); }

.savanna-styles .callout { display: flex; align-items: center; background-color: rgb(231, 233, 236); position: relative; margin-bottom: 15px; }

.savanna-styles .callout h { color: rgb(86, 72, 183); }

.savanna-styles .callout::before { content: ""; width: 10px; height: 100%; background-color: rgb(86, 72, 183); position: absolute; top: 0px; }

.savanna-styles .callout-content { flex: 1 1 0%; padding: 20px 20px 20px 30px; }

.savanna-styles .video-wrapper { position: relative; width: 100%; padding-bottom: 56.25%; height: 0px; }

.savanna-styles .video-wrapper iframe { position: absolute; top: 0px; left: 0px; width: 100%; height: 100%; }

.savanna-styles .socials-element { color: rgb(0, 43, 86); box-shadow: rgba(0, 0, 0, 0.12) 0px 1px 3px, rgba(0, 0, 0, 0.24) 0px 1px 2px; border-radius: 5px; padding: 15px; display: flex; flex-direction: column; align-items: center; }

.savanna-styles .btn-socials { position: relative; display: flex; justify-content: center; align-items: center; overflow: hidden; cursor: pointer; width: 210px; height: 50px; background-color: rgb(204, 213, 221); border-radius: 80px; padding: 0px 5px; will-change: transform; transition: all 0.2s ease-in-out 0s; }

.savanna-styles .btn-socials:hover { transform: scale(1.1); }

.savanna-styles .btn-socials:hover span { transition-delay: 0.25s; transform: translateX(-280px); }

.savanna-styles .btn-socials:hover i { opacity: 1; transform: scale(1); }

.savanna-styles .btn-socials i { height: 20px; width: 100%; opacity: 0; color: rgb(0, 43, 86); will-change: transform; transform: scale(0.1); transition: all 0.3s ease 0s; }

.savanna-styles .btn-socials i:hover { animation: 1s ease 0s infinite alternate none running pulse; }

.savanna-styles .btn-socials .alx-icon { content: url("https://raw.githubusercontent.com/Explore-AI/Pictures/master/alx-courses/aice/assets/ALX_Navy.png"); }

.savanna-styles .btn-socials .twitter-icon { content: url("https://raw.githubusercontent.com/Explore-AI/Pictures/master/alx-courses/aice/assets/square-x-twitter.png"); }

.savanna-styles .btn-socials .linkedin-icon { content: url("https://raw.githubusercontent.com/Explore-AI/Pictures/master/alx-courses/aice/assets/linkedin.png"); }

.savanna-styles .btn-socials .instagram-icon { content: url("https://raw.githubusercontent.com/Explore-AI/Pictures/master/alx-courses/aice/assets/square-instagram.png"); }

.savanna-styles .btn-socials .facebook-icon { content: url("https://raw.githubusercontent.com/Explore-AI/Pictures/master/alx-courses/aice/assets/square-facebook.png"); }

.savanna-styles .btn-socials span { position: absolute; z-index: 99; width: 210px; height: 50px; border-radius: 80px; font-family: Poppins, "sans-serif"; font-size: 14px; text-align: center; line-height: 50px; letter-spacing: 2px; color: rgb(255, 255, 255); background-color: rgb(0, 43, 86); padding: 0px 18px; transition: all 1.2s ease 0s; }

.savanna-styles .btn-socials .container { display: flex; justify-content: space-around; align-items: center; width: 100%; height: 100%; border-radius: 80px; }

.savanna-styles .btn-socials .container a { display: flex; justify-content: center; align-items: center; }

.savanna-styles .btn-secondary { cursor: pointer; width: 210px; height: 50px; border-radius: 80px; padding: 0px 5px; will-change: transform; transition: all 0.2s ease-in-out 0s; color: rgb(0, 43, 86); background-color: rgb(5, 242, 131); font-family: Poppins, "sans-serif"; font-size: 14px; text-align: center; outline: none; border: none; }

.savanna-styles .btn-secondary:hover { transform: scale(1.1); }

.savanna-styles summary { list-style: none; display: flex; justify-content: space-between; padding: 15px; border-radius: 5px; align-items: center; line-height: 1.33; }

.savanna-styles summary div { flex: 1 1 0%; max-width: calc(100% - 25px); overflow: hidden; }

.savanna-styles summary::after { content: ""; width: 15px; height: 15px; background-size: cover; margin-left: 0.75em; }

.savanna-styles details[open] > summary::after { transform: rotate(45deg); animation: 0.2s ease 0s 1 normal none running rotate; }

.savanna-styles summary:hover::after { animation: 1s ease 0s infinite alternate none running pulse; }

.savanna-styles details { padding-left: 0px; padding-bottom: 5px; cursor: pointer; }

.savanna-styles .task-summary { background-color: rgb(5, 242, 131); color: rgb(0, 43, 86); }

.savanna-styles .task-summary:hover { background-color: rgb(55, 245, 156); }

